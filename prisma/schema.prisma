generator client {
  provider        = "prisma-client-js"
  output          = "../models/prisma"
  previewFeatures = ["multiSchema"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  schemas  = ["laporan", "public"]
}

model export_proses {
  id_export_proses Int       @id @default(autoincrement())
  date             DateTime  @db.Timestamptz(6)
  path             String?   @db.VarChar(255)
  filename         String?   @db.VarChar(255)
  type             String    @db.VarChar(255)
  status           Int
  status_note      String    @db.VarChar(255)
  total_data       Int?
  createdBy        Int
  createdName      String    @db.VarChar(255)
  createdCode      String    @db.VarChar(255)
  updatedBy        Int?
  updatedName      String?   @db.VarChar(255)
  updatedCode      String?   @db.VarChar(255)
  deletedBy        Int?
  deletedName      String?   @db.VarChar(255)
  deletedCode      String?   @db.VarChar(255)
  createdAt        DateTime  @db.Timestamptz(6)
  updatedAt        DateTime  @db.Timestamptz(6)
  deletedAt        DateTime? @db.Timestamptz(6)
  meta_data        Json?     @db.Json

  @@schema("laporan")
}

model laporan_view {
  id_laporan_view  Int       @id @default(autoincrement())
  jenis            String    @db.VarChar(255)
  nama             String
  kode             String    @unique(map: "laporan_view_unique") @db.VarChar(255)
  filter           Json?     @db.Json
