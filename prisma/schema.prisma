  source_attribute String
  source_total     String
  field            Json      @db.Json
  status           Boolean   @default(true)
  keterangan       String?
  createdBy        Int
  createdName      String    @db.VarChar(255)
  createdCode      String    @db.VarChar(255)
  updatedBy        Int?
  updatedName      String?   @db.VarChar(255)
  updatedCode      String?   @db.VarChar(255)
  deletedBy        Int?
  deletedName      String?   @db.VarChar(255)
  deletedCode      String?   @db.VarChar(255)
  createdAt        DateTime  @db.Timestamptz(6)
  updatedAt        DateTime  @db.Timestamptz(6)
  deletedAt        DateTime? @db.Timestamptz(6)
  source_order     String    @default(" ")
  is_filter        Boolean   @default(true)
  is_export        Boolean   @default(true)
  rules            Json      @default("{\"max_data\":null,\"max_interval_day\":null}") @db.Json
  filter_cte       Json?     @db.Json
  is_api           Boolean   @default(false)

  @@schema("laporan")
}

