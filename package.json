    "@prisma/client": "^6.5.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "connect-redis": "^8.0.2",
    "dotenv": "^16.4.7",
    "express-session": "^1.18.1",
    "helmet": "^8.1.0",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.2",
    "nest-winston": "^1.10.2",
    "puppeteer": "^24.4.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "winston": "^3.17.0"
